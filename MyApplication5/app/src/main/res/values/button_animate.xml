<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="button_animate">
        <attr name="buttonColor" format="color"/>
        <attr name="cricleColor" format="color"/>
        <attr name="textColor" format="color"/>
        <attr name="mWdith" format="dimension"/>
        <attr name="mHeight" format="dimension"/>
        <attr name="text" format="string"/>
        <attr name="button_layout">
            <enum name="content" value="0"/>
            <enum name="left" value="1"/>
            <enum name="right" value="2"/>
        </attr>
        <attr name="ImageSrc" format="reference"/>
    </declare-styleable>
</resources>
